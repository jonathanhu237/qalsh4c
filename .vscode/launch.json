    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb-dap",
            "request": "launch",
            "name": "Debug QALSH ANN Estimator (In Memory)",
            "program": "${workspaceRoot}/build/qalsh_chamfer",
            "args": [
                "estimate",
                "-d",
                "./data/toy",
                "--in-memory",
                "ann",
                "qalsh"
            ],
            "env": [],
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "lldb-dap",
            "request": "launch",
            "name": "Debug QALSH Sampling Estimator (In Memory)",
            "program": "${workspaceRoot}/build/qalsh_chamfer",
            "args": [
                "-l",
                "debug",
                "estimate",
                "-d",
                "./data/toy",
                "--in-memory",
                "sampling",
                "qalsh"
            ],
            "env": [],
            "cwd": "${workspaceRoot}"
        }
    ]
}